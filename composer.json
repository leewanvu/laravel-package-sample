{
    "name": "leewanvu/laravel-package-sample",
    "description": "Laravel package sample",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vu Le",
            "email": "leewanvu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*|5.7.*|5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Leewanvu\\LaravelPackageSample\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leewanvu\\LaravelPackageSample\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leewanvu\\LaravelPackageSample\\LaravelPackageSampleServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
